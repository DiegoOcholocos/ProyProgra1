@model IEnumerable<appproy.Models.Pagos>

@{
    ViewData["Title"] = "Pagos";
}


<h1>@ViewData["Title"]</h1>

<a class="nav-link text-dark" asp-area="" asp-controller="Pagos" asp-action="vistaP"><b>Vista2.0</b></a>

<table class="table">

    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.NOMBRES_Y_APELLIDOS)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DNI)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Celular)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Computacion_info)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Confeccion_info)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Estetica_info)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Turno)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Monto)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Codigo_recibo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Foto_recibo)
            </th>
            <th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.NOMBRES_Y_APELLIDOS)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DNI)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Celular)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Computacion_info)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Confeccion_info)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Estetica_info)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Turno)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Monto)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Codigo_recibo)
            </td>
            <td>
                <img class="img-responsive" width="500" height="500"
                       src="@Html.DisplayFor(modelItem => item.Foto_recibo)" />
            </td>

            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>