                <span asp-validation-for="Confeccion_info" class="text-danger"></span>
            </div>
               <div class="form-group">
                <label asp-for="Estetica_info" class="control-label"></label>
                <input asp-for="Estetica_info" class="form-control" />
                <span asp-validation-for="Estetica_info" class="text-danger"></span>
            </div>
