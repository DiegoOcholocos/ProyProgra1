            </div>
               <div class="form-group">
                <label asp-for="Foto_recibo" class="control-label"></label>
                <input asp-for="Foto_recibo" class="form-control" />
                <span asp-validation-for="Foto_recibo" class="text-danger"></span>
            </div>
               <div class="form-group">
                <label asp-for="Status" class="control-label"></label>
                <input asp-for="Status" class="form-control" />
