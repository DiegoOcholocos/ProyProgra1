@model dynamic;
@using System.Collections.Generic;

@{
    ViewData["Title"] = "Matriculas";
    IEnumerable<appproy.Models.UsersMatricula> listItemsMatricula = new List<UsersMatricula>();
    listItemsMatricula = @Model.elementosDatos;
}

<h1>@ViewData["Title"]</h1>
<a class="nav-link text-dark" asp-area="" asp-controller="UsersMatricula" asp-action="Create"><b>NUEVO</b></a>
<table class="table">
    <thead>
        <tr>
            <th>
                Nombre
            </th>
            <th>
                Apellidos
            </th>
            <th>
                @Html.DisplayNameFor(model => listItemsMatricula.FirstOrDefault().DNI)
            </th>
            <th>
                @Html.DisplayNameFor(model => listItemsMatricula.FirstOrDefault().Pasaporte)
            </th>
            <th>
                @Html.DisplayNameFor(model => listItemsMatricula.FirstOrDefault().Carnet_de_extranjeria)
            </th>
            <th>
                @Html.DisplayNameFor(model => listItemsMatricula.FirstOrDefault().Nacionalidad)
            </th>
            <th>
                @Html.DisplayNameFor(model => listItemsMatricula.FirstOrDefault().Mes)
            </th>
            <th>
                @Html.DisplayNameFor(model => listItemsMatricula.FirstOrDefault().Edad)
            </th>
            <th>
                @Html.DisplayNameFor(model => listItemsMatricula.FirstOrDefault().Celular)
            </th>
            <th>
                @Html.DisplayNameFor(model => listItemsMatricula.FirstOrDefault().Operador)
            </th>
            <th>
                @Html.DisplayNameFor(model => listItemsMatricula.FirstOrDefault().Sexo)
            </th>
            <th>
                @Html.DisplayNameFor(model => listItemsMatricula.FirstOrDefault().Grado_Academico)
            </th>
            <th>
                @Html.DisplayNameFor(model => listItemsMatricula.FirstOrDefault().Correo)
            </th>
            <th>
                @Html.DisplayNameFor(model => listItemsMatricula.FirstOrDefault().Direccion)
            </th>
            <th>
                @Html.DisplayNameFor(model => listItemsMatricula.FirstOrDefault().Distrito)
            </th>
            <th>
                @Html.DisplayNameFor(model => listItemsMatricula.FirstOrDefault().Vacuna)
            </th>
            <th>
                @Html.DisplayNameFor(model => listItemsMatricula.FirstOrDefault().Computacion_info)
            </th>
            <th>
                @Html.DisplayNameFor(model => listItemsMatricula.FirstOrDefault().Confeccion_info)
            </th>
            <th>
                @Html.DisplayNameFor(model => listItemsMatricula.FirstOrDefault().Estetica_info)
            </th>
            <th>
                @Html.DisplayNameFor(model => listItemsMatricula.FirstOrDefault().Horario)
            </th>
            <th>
                @Html.DisplayNameFor(model => listItemsMatricula.FirstOrDefault().Foto_DNI_Cara)
            </th>
            <th>
                @Html.DisplayNameFor(model => listItemsMatricula.FirstOrDefault().Foto_DNI_Sello)
            </th>
            <th>
                @Html.DisplayNameFor(model => listItemsMatricula.FirstOrDefault().Codigo_Voucher)
            </th>
            <th>
                @Html.DisplayNameFor(model => listItemsMatricula.FirstOrDefault().Foto_Voucher)
            </th>
            <th>
                @Html.DisplayNameFor(model => listItemsMatricula.FirstOrDefault().Status)
            </th>
            <th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (UsersMatricula item in Model.elementosDatos) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LastName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DNI)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Pasaporte)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Carnet_de_extranjeria)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Nacionalidad)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Mes)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Edad)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Celular)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Operador)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Sexo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Grado_Academico)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Correo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Direccion)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Distrito)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Vacuna)
            </td>


            <td>
                @Html.DisplayFor(modelItem => item.Computacion_info)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Confeccion_info)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Estetica_info)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Horario)
            </td>
            <td>
                <img class="img-responsive" width="500" height="500"
                       src="@Html.DisplayFor(modelItem => item.Foto_DNI_Cara)" />
            </td>
            <td>
                <img class="img-responsive" width="500" height="500"
                       src="@Html.DisplayFor(modelItem => item.Foto_DNI_Sello)" />
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Codigo_Voucher)
            </td>
            <td>
                <img class="img-responsive" width="500" height="500"
                       src="@Html.DisplayFor(modelItem => item.Foto_Voucher)" />
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Status)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>